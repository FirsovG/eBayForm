<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:eBayForm.DesignItems">
    
    <Canvas x:Key="Apple" Height="35" Width="35">
        <Ellipse Height="35" Width="35">
            <Ellipse.Fill>
                <RadialGradientBrush  Center="0.4,0.4" RadiusX="1" RadiusY="1">
                    <GradientStop Color="Black" Offset="0.4"/>
                    <GradientStop Color="White" Offset="0.9"/>
                </RadialGradientBrush>
            </Ellipse.Fill>
        </Ellipse>
    </Canvas>

    <Canvas x:Key="Samsung" Height="20" Width="20">
        <Line Stroke="#dddddd" StrokeThickness="2.5" X1="2.5" Y1="2.5" X2="2.5" Y2="17.5" StrokeStartLineCap="Round" StrokeEndLineCap="Round"/>
        <Line Stroke="#dddddd" StrokeThickness="2.5" X1="2.5" Y1="2.5" X2="17.5" Y2="2.5" StrokeStartLineCap="Round" StrokeEndLineCap="Round"/>

        <Line Stroke="#dddddd" StrokeThickness="1.75" X1="17.5" Y1="17.5" X2="6" Y2="17.5" StrokeStartLineCap="Round" StrokeEndLineCap="Round"/>
        <Line Stroke="#dddddd" StrokeThickness="1.75" X1="17.5" Y1="17.5" X2="17.5" Y2="6" StrokeStartLineCap="Round" StrokeEndLineCap="Round"/>
    </Canvas>

</ResourceDictionary>